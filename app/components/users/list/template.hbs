<table class='table table-striped'>
  <thead>
    <tr>
      <th scope='col'>Id</th>
      <th scope='col'>Username</th>
      <th scope='col'>Password</th>
      <th scope='col'>Email</th>
      <th scope='col'>Memory top score</th>
    </tr>
  </thead>
  <tbody>
    {{#each @users as |user|}}
      <tr>
        <td>{{user.id}}</td>
        <td><LinkTo @route='home.user.show'>{{user.username}}</LinkTo></td>
        <td>{{user.password}}</td>
        <td>{{user.email}}</td>
        <td>{{user.memoryTopScore}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>